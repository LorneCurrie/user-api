{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf ./build/ && tsc",
    "prestart": "yarn build",
    "start": "node ./bin/www",
    "dev": ". ./env.dev.sh && ./node_modules/nodemon/bin/nodemon.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "db-migration:dev": "./node_modules/.bin/knex --knexfile ./db/knexfile.js migrate:latest --env develop"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "cookie-parser": "1.4.4",
    "debug": "2.6.9",
    "express": "4.16.1",
    "http-errors": "1.6.3",
    "knex": "^0.20.3",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "mysql2": "^2.0.1",
    "objection": "^1.6.11",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.14",
    "nodemon": "^2.0.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2"
  }
}
